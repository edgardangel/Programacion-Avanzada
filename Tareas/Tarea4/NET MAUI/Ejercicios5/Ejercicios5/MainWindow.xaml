<Window x:Class="Ejercicios5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ejercicios5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Label Content="Nombre" Canvas.Left="34" Canvas.Top="38" Height="33" Width="224" Background="#FF46D5E6" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Segoe UI Semibold" FontSize="18"/>
        <Label Content="Titulo" Canvas.Left="34" Canvas.Top="94" Height="33" Width="224" Background="#FF46D5E6" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Segoe UI Semibold" FontSize="18"/>
        <Label Content="DOB" Canvas.Left="34" Canvas.Top="158" Height="33" Width="224" Background="#FF46D5E6" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Segoe UI Semibold" FontSize="18"/>
        <TextBox Canvas.Left="288" TextWrapping="Wrap" Text=" " Canvas.Top="38" Width="224" Height="33" TextChanged="TextBox_TextChanged" Background="#FF35FF05" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Segoe UI Semibold" FontSize="18"/>
        <TextBox Canvas.Left="288" TextWrapping="Wrap" Text=" " Canvas.Top="94" Width="224" Height="33" TextChanged="TextBox_TextChanged" Background="#FF35FF05" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Segoe UI Semibold" FontSize="18"/>
        <Button Content="Agregar" Canvas.Left="34" Canvas.Top="207" HorizontalAlignment="Left" VerticalAlignment="Center" Width="224" Height="41" Click="Button_Click" FontFamily="Segoe UI Semibold" FontSize="18"/>
        <DatePicker Canvas.Left="288" Canvas.Top="158" Height="33" Width="224" FontFamily="Segoe UI Semibold" FontSize="18" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Button Content="Clear" Canvas.Left="288" Canvas.Top="207" Width="224" Height="41" Click="Button_Click" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Segoe UI Semibold" FontSize="18"/>
        <ListBox Height="386" Width="259" Canvas.Left="531" Canvas.Top="38" SelectionChanged="ListBox_SelectionChanged" HorizontalAlignment="Center" VerticalAlignment="Top">
            <ListBoxItem Content="Articulo 1"/>
            <ListBoxItem Content="Articulo 2"/>
        </ListBox>
        <Ellipse Height="100" Canvas.Left="363" Canvas.Top="257" Width="100" OpacityMask="#FFF10000" Fill="Red" Stroke="Red"/>
        <Ellipse Height="100" Canvas.Left="363" Canvas.Top="324" Width="100" OpacityMask="#FF14F100" Fill="#FF56FA03" Stroke="#FFA9FF00" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Calendar Canvas.Left="0" Canvas.Bottom="0"/>
        <TextBlock Text="TextBlock" Canvas.Top="282" Canvas.Left="259"/>
    </Canvas>
</Window>
